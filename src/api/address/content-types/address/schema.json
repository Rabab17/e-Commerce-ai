{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "singularName": "address",
    "pluralName": "addresses",
    "displayName": "Address"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "street": {
      "type": "text",
      "required": true,
      "minLength": 5,
      "maxLength": 200,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "city": {
      "type": "text",
      "required": true,
      "minLength": 2,
      "maxLength": 100,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "state": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "postalCode": {
      "type": "string",
      "required": true,
      "minLength": 5,
      "maxLength": 10,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "country": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 50,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "isDefault": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "addresses"
    },
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order",
      "inversedBy": "address"
    }
  }
}
